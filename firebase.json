{
  "hosting": {
    "public": "build",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "rewrites": [
      {"source": "/report", "function": "report"},
       {"source": "/forzaAggiornaMagazzino", "function": "forzaAggiornaMagazzino"},
       {"source": "/forzaAggiornaTitoli", "function": "forzaAggiornaTitoli"},
  
     
      {
        "source": "**",
        "destination": "/index.html"
      }
    ]
  }
}
